<?xml version="1.0" encoding="UTF-8"?>

<!-- When you kill Flagship's crew, it turns off oxygen and medbay since it does not need them anymore -->

<mod:findName type="event" name="BOSS_AUTOMATED">
	<mod-append:status type="limit" target="enemy" system="medbay" amount="0"/>
	<mod-append:status type="limit" target="enemy" system="oxygen" amount="0"/>
</mod:findName>

